<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.take2.take2.take2"
    android:background="#e1c911"
    android:clickable="false"
    android:paddingEnd="@dimen/abc_action_bar_default_padding_start_material">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cyklo"
        android:id="@+id/textView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rent a Cycle "
        android:id="@+id/button1"
        android:layout_marginTop="114dp"
        android:layout_below="@+id/textView"
        android:onClick="Rent A Cycle"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Book a cycle"
        android:id="@+id/button2"
        android:onClick="Book a cycle"
        android:layout_below="@+id/button1"
        android:layout_alignStart="@+id/button1"
        android:layout_marginTop="94dp" />
</RelativeLayout>
